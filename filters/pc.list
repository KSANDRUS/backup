# Exclude from home
-home/dragon/.cache/*
-home/dragon/.nv/*
-home/dragon/.local/share/Steam/*
-home/dragon/.local/share/lutris/*
-home/dragon/.local/share/TelegramDesktop/tdata/user_data/*cache/*
-home/dragon/.tor-browser/*

# Exclude Chromium caches (mostly from Electron apps)
-*/Cache/*
-*/GPUCache/*
-*/CachedData/*

# Exclude Gradle build folders
-home/dragon/code/java/*/build/*

# Exclude ROM build artifactss
-home/dragon/code/android/aosp/out/*
-home/dragon/code/android/los/*

# Include backup information
+home/dragon/backup/info/*

# Include the rest of home
+home/dragon/*
+home/

# Include some system configuration
+boot/loader/*
+boot/EFI/*
+boot/shellx64.efi
+boot/
-etc/ld.so.cache
+etc/*

# Include libvirt state
+var/lib/libvirt/*

# Include Pacman database
+var/lib/pacman/local/*
+var/lib/pacman/
+var/lib/
+var/

# Exclude everything else
-* 
